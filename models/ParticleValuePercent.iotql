CREATE MODEL ParticleValuePercent WITH
    schema:name = "particle-value-percent"
ATTRIBUTE value WITH
    schema:name = "value",
    iot:purpose = iot-purpose:value,
    iot:type = iot:type.integer,
    iot:unit = iot-unit:math.fraction.percent,
    iot:minimum = 0,
    iot:maximum = 100,
    iot:read = false,
    iot:write = true,
    iot:actuator = true
;
